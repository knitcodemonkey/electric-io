{
  "name": "electric-io",
  "version": "0.1.0",
  "description": "an IoT Dashboard for Azure IoT Hub",
  "author": "Suz Hinton",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch",
    "watch": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --progress --colors --watch",
    "watch:server": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "precommit": "pretty-quick --staged",
    "build": "webpack --progress --colors"
  },
  "dependencies": {
    "@azure/event-hubs": "^1.0.8",
    "@babel/runtime": "^7.3.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.2",
    "azure-iothub": "1.9.3",
    "body-parser": "^1.18.3",
    "chart.js": "2.7.3",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-sanitizer": "^1.0.5",
    "marked": "0.6.0",
    "socket.io": "2.2.0",
    "vue": "2.6.3",
    "vue-focus": "2.1.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "engineStrict": true,
  "keywords": [
    "node",
    "express",
    "iot"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-stage-2": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "css-loader": "^2.1.0",
    "debug": "^4.1.1",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "puppeteer": "^1.12.2",
    "vue-jest": "^4.0.0-beta.2",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.6.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-watch-server": "^1.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "public/js/**/*.js",
      "!public/js/dist/**",
      "!**/node_modules/**"
    ],
    "roots": [
      "lib",
      "public/js"
    ],
    "setupFiles": [
      "./lib/testSetup.js"
    ],
    "testURL": "http://localhost/",
    "husky": {
      "hooks": {
        "pre-commit": "pretty-quick --staged"
      }
    }
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "public/*"
    ]
  }
}
